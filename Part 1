function setup() {
  createCanvas(500,500);
}

function draw() {
  background(0);
  array();
  
  function array(){
   for (var x = 10; x<= width; x = x+ 50){
      for (var y = 10; y<= height; y = y+ 50){
        fill(random(255), 0, random(255));
        rect(x,y,25,25);
      }
    }
    for (var x = 11; x<= width; x = x+ 50){
      for (var y = 11; y<= height; y = y+ 50){
        fill(random(255), 0, random(255));
        ellipse(x,y,25,25);
      }
    }
  }
}
